# React App Environment Variables

# WebSocket Configuration - permite hot reload fără erori
WDS_SOCKET_HOST=localhost
WDS_SOCKET_PORT=3000
WDS_SOCKET_PATH=/ws

# Fast Refresh activat
FAST_REFRESH=true

# Browser settings
BROWSER=none
HTTPS=false

# Suprimate warnings
DISABLE_ESLINT_PLUGIN=true
ESLINT_NO_DEV_ERRORS=true
GENERATE_SOURCEMAP=false
